{
  "name": "ai-image-detector",
  "version": "0.0.1",
  "description": "Analyzes an input image and guesses if it is AI generated.",
  "main": "./src/{index.js,functions/*.js}",
  "type": "module",
  "types": "./src/types.d.ts",
  "scripts": {
    "start-azurite": "npx -y azurite --silent",
    "start-azure-function": "func start",
    "start": "run-p -r start-azurite start-azure-function",
    "demo": "node src/demo.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "eslint --fix ./src && prettier -w ./src"
  },
  "keywords": [
    "ai",
    "image",
    "detector",
    "generated"
  ],
  "author": "haywoodsloan",
  "license": "ISC",
  "dependencies": {
    "@azure/communication-email": "^1.0.0",
    "@azure/functions": "^4.5.0",
    "@azure/identity": "^4.3.0",
    "@huggingface/inference": "^2.7.0",
    "common": "file:../common",
    "email-validator": "^2.0.4",
    "memoize": "^10.0.0",
    "mongodb": "^6.7.0",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.6.0",
    "mongodb-memory-server": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2"
  },
  "engines": {
    "node": ">=20"
  }
}
