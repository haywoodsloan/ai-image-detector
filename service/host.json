{
  "version": "2.0",
  "functionTimeout": "00:00:45",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "durableTask": {
      "hubName": "%HUB_NAME%",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      }
    },
    "http": {
      "routePrefix": ""
    }
  }
}