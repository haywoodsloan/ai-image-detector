{
  "name": "ai-image-detector-extension",
  "description": "Checks for AI generated images and incorporates community feedback",
  "version": "1.2.1",
  "types": "./types.d.ts",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "format": "eslint --fix ./components ./entrypoints && prettier -w ./components ./entrypoints",
    "gen-icon": "icon-gen -i ./assets/detector.svg -o ./public/icon/ -r --favicon --favicon-name / --favicon-png-sizes 16,24,32,48,96,128"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.5",
    "@mdi/js": "^7.4.47",
    "@vueuse/core": "^13.2.0",
    "clone-deep": "^4.0.1",
    "color-interpolate": "^1.0.5",
    "common": "file:../common",
    "email-validator": "^2.0.4",
    "hash-wasm": "^4.12.0",
    "memoize": "^10.1.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.14",
    "vuetify": "^3.8.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@wxt-dev/module-vue": "^1.0.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "icon-gen": "^5.0.0",
    "postcss-prefix-selector": "^2.1.1",
    "postcss-rem-to-pixel": "^4.1.2",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10",
    "wxt": "^0.20.6"
  }
}
