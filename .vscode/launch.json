  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Data Scrapper (AI)",
      "request": "launch",
      "runtimeArgs": ["run-script", "start", "--", "--count", "1000000"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/scrapper"
    },
    {
      "name": "Run Data Scrapper (Real Art)",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start",
        "--",
        "--count",
        "1000000",
        "--real"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/scrapper"
    },
    {
      "name": "Run Data Scrapper (Real Pics)",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start",
        "--",
        "--count",
        "1000000",
        "--real",
        "--pics"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/scrapper"
    },
    {
      "name": "Run Service Demo",
      "request": "launch",
      "runtimeArgs": ["run-script", "demo"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/service"
    }
  ]
}
