{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Image Processor",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/processor/process_images.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Run Data Scrapper (AI)",
      "request": "launch",
      "runtimeArgs": ["run-script", "start", "--", "--debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/scrapper"
    },
    {
      "name": "Run Data Scrapper (Real)",
      "request": "launch",
      "runtimeArgs": ["run-script", "start", "--", "--real", "--debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/scrapper"
    },
    {
      "name": "Run Service Demo",
      "request": "launch",
      "runtimeArgs": ["run-script", "demo"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/service"
    }
  ]
}
